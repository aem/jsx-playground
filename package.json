  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "deploy": "npm run clean & npm run build && surge",
    "start": "cross-env NODE_ENV=development node server.js",
    "example0": "babel-node --plugins transform-react-jsx assets/example0.js",
    "example0-print": "babel --plugins transform-react-jsx assets/example0.js",
    "example1": "babel-node --plugins transform-react-jsx assets/example1.js",
    "example1-print": "babel --plugins transform-react-jsx assets/example1.js",
    "example2": "babel-node --plugins transform-react-jsx assets/example2.js",
    "example2-print": "babel --plugins transform-react-jsx assets/example2.js",
    "example3": "babel-node --plugins=transform-react-jsx,transform-object-rest-spread assets/example3.js",
    "example3-print": "babel --plugins=transform-react-jsx,transform-object-rest-spread assets/example3.js",
    "example4": "babel-node --plugins transform-react-jsx assets/example4.js",
    "example4-print": "babel --plugins transform-react-jsx assets/example4.js",
    "example5": "babel-node --plugins transform-react-jsx assets/example5.js",
    "example5-print": "babel --plugins transform-react-jsx assets/example5.js"
  },
